E <- function(u,v){
(u^(3) * exp(v-2) - 4*v^(3)*exp(-u))^2
}
dE <- function(u,v){
Eu <- 2 * ( ((u^3)*exp(v-2) - 4* exp(-u)*v^3) * (3*u^(2)*exp(v-2) + 4*exp(-u)*v^3 ) )
Ev <- 2 * ( ((u^3)*exp(v-2) - 12*exp(-u)*v^2) * (  u^(3)*exp(v-2) - 4*exp(-u)*v^3 ) )
c(Eu, Ev)
}
GD <- function(f, f_prima, t_aprendizaje = 0.1, umbral = 10^(-4), nIter=500, pInicial=c(1,1)){
w_anterior <- c(0,0)
w_nuevo <- pInicial
