# Practica 1 | Programación
#
# Por: Antonio Miguel Morillo Chica
#
# 1. Búsqueda iteractiva de óptimos
# 1.1 Implementar el algoritmo de gradiante descendente.
# ------------------------------------------------------------------------
E <- function(u,v){
(u^(3) * exp(v-2) - 4*v^(3)*exp(-u))^2
}
dE <- function(u,v){
Eu <- 2 * ( (u^2)*exp(v) - 2*(v^2)*exp(-u) ) * ( 2*exp(v)*u + 2*exp(-u)*(v^2) )
Ev <- 2 * ( (u^2)*exp(v) - 2*(v^2)*exp(-u) ) * ( (u^2)*exp(v) - 4*exp(-u)*v )
c(Eu, Ev)
}
GD <- function(f, f_prima, t_aprendizaje = 0.1, umbral = 10^(-4), nIter=500, pInicial=c(0,0)){
w_anterior <- c(0,0)
w_nuevo <- pInicial
iters <- 0
datos <- rep(nIter)
datos[iters] <- f(w_nuevo[1], w_nuevo[2])
cat("Inicio: ", w_nuevo[1], w_nuevo[2])
cat("\ng(inicial): ", f_prima(w_nuevo[1],w_nuevo[2]))
cat("\nf(inicial): ", f(w_nuevo[1], w_nuevo[2]))
while( (iters <= nIter) & ( f(w_nuevo[1],w_nuevo[2]) > umbral)
& (abs(f(w_nuevo[1], w_nuevo[2]) - f(w_anterior[1], w_anterior[2])) > umbral) ) {
w_anterior = w_nuevo
gradiante <- f_prima(w_nuevo[1], w_nuevo[2])
n_gradiante <- gradiante * (-1)
w_nuevo <- w_anterior + t_aprendizaje*n_gradiante
iters <- iters + 1
datos[iters] <- f(w_nuevo[1], w_nuevo[2])
}
cat("\ng(final): ", f_prima(w_nuevo[1],w_nuevo[2]))
cat("\nf(inicial): ", f(w_nuevo[1], w_nuevo[2]))
cat("\nSolución: ", w_nuevo[1], w_nuevo[2])
list(valores = datos[1:nIter])
}
## 1.2 Considerar la función E(u,v) = (u³e^(v-2) - 4v³e^(-u))². Usar el gradiante descendente para encontrar un mínimo de esta función, comenzando desde el punto (u, v) = (1, 1) y usando una tasa de aprendizaje η = 0,1.
GD(f=E, f_prima=dE, t_aprendizaje=0.1, umbral= 10^(-4), nIter=500, pInicial=c(1,1))
## a) Calcular analiticamente y mostrar la expresión del gradiante de la función E(u,v)
## b) ¿Cuántas iteraciones tarda el algoritmo en obtener por primera vez un valor de E(u, v) inferior a 10^−14 . (Usar flotantes de 64 bits)
## c) ¿En qué coordenadas (u, v) se alcanzó por primera vez un valor igual o menor a 10^−14 en el apartado anterior.
